<Window x:Class="KAPEYAP_KURS.MainMenuWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KAPEYAP_KURS"
        mc:Ignorable="d"
        Title="MainMenuWin" Height="351" Width="239.5">
    <Grid>
        <StackPanel Height="65" Margin="26,10,26,0" VerticalAlignment="Top" Background="#FFFFD76D">
            <Button x:Name="PersonCalcB" Content="Расчет ЗП" VerticalAlignment="Top" Height="54" FontSize="14" Click="PersonCalcB_Click" Margin="5,5,6,5"/>
        </StackPanel>
        <Button Content="Выйти" HorizontalAlignment="Left" Margin="10,262,0,0" VerticalAlignment="Top" Width="100" Height="48" FontFamily="Segoe UI Black" Foreground="White" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource ="out.jpg"/>
            </Button.Background>
        </Button>
        <StackPanel Height="65" Margin="26,80,26,0" VerticalAlignment="Top" Background="#FFFFD76D">
            <Button x:Name="FAQB1" Content="Справка" VerticalAlignment="Top" Height="54" FontSize="14" Click="FAQB_Click" Margin="5,5,6,5"/>
        </StackPanel>

    </Grid>
</Window>
